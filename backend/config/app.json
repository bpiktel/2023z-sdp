{
    "tracing": {
        "directives": "backend=trace,tower_http=debug",
        "console": true,
        "json": "logs"
    },
    "app": {
        "url": "0.0.0.0:3000",
        "cors": false
    },
    "auth_keys": {
        "encoding": "devkeys/priv.pem",
        "decoding": "devkeys/pub.pem"
    },
    "surreal_db": {
        "address": "ws://127.0.0.1:8000",
        "namespace": "default",
        "database": "default",
        "migrations": {
            "directory": "migrations"
        }
    },
    "file_storage": {
        "folder": "storage"
    }
}